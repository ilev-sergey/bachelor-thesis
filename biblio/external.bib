  author       = {Chernikova, Anna G. and Kozodaev, Maxim G. and Negrov, Dmitry V. and Korostylev, Evgeny V. and Park, Min Hyuk and Schroeder, Uwe and Hwang, Cheol Seong and Markeev, Andrey M.},
