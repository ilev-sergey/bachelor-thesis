  date         = {2023-12-01},
  file         = {Full Text:C\:\\Users\\Sergey\\Zotero\\storage\\KYITYY75\\Lee et al. - 2023 - Role of oxygen vacancies in ferroelectric or resis.pdf:application/pdf}
}

