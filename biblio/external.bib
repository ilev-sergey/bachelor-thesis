  number       = {1},
  journaltitle = {Journal of Vacuum Science \& Technology B, Nanotechnology and Microelectronics: Materials, Processing, Measurement, and Phenomena},
  author       = {Lin, Yuh-Chen and {McGuire}, Felicia and Franklin, Aaron D.},
