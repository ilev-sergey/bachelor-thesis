@article{chouprikEffectDomainStructure2023,
  title        = {Effect of {{Domain Structure}} and {{Dielectric Interlayer}} on {{Switching Speed}} of {{Ferroelectric Hf0}}.{{5Zr0}}.{{5O2 Film}}},
  author       = {Chouprik, Anastasia and Savelyeva, Ekaterina and Korostylev, Evgeny and Kondratyuk, Ekaterina and Zarubin, Sergey and Sizykh, Nikita and Zhuk, Maksim and Zenkevich, Andrei and Markeev, Andrey M. and Kondratev, Oleg and Yakunin, Sergey},
