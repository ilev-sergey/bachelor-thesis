@article{stolichnovGenuinelyFerroelectricSub1VoltSwitchable2018,
  title        = {Genuinely {{Ferroelectric Sub-1-Volt-Switchable Nanodomains}} in {{HfxZr}}(1–x){{O2 Ultrathin Capacitors}}},
  author       = {Stolichnov, Igor and Cavalieri, Matteo and Colla, Enrico and Schenk, Tony and Mittmann, Terence and Mikolajick, Thomas and Schroeder, Uwe and Ionescu, Adrian M.},
  date         = {2018-09-12},
  journaltitle = {ACS Applied Materials \& Interfaces},
  shortjournal = {ACS Appl. Mater. Interfaces},
