  title        = {Thermophysical and mechanical properties of cubic, tetragonal and monoclinic {ZrO}2},
  volume       = {23},
  issn         = {22387854},
  url          = {https://linkinghub.elsevier.com/retrieve/pii/S2238785423000406},
